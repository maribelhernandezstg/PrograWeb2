generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  name      String
  lastName  String
  email     String   @unique
  active    Boolean  @default(true)
  avatar    String
  createdAt DateTime @default(now())
  posts     Post[]
}








